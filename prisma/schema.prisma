generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DuePayment {
  id        String   @id @default(cuid())
  title     String
  amount    Float
  dueDate   DateTime
  status    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
